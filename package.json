  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "npm run postcss:build && ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "postcss:build": "postcss ./app/styles/tailwind-input.css -o ./public/assets/tailwind.css",
    "postcss:watch": "postcss ./app/styles/tailwind-input.css -o ./public/assets/tailwind.css --watch --verbose",
    "release": "release-it",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test"
  },
